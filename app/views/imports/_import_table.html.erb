<table class="import-table bordered striped responsive-table">
  <thead>
    <tr>
      <th>File</th>
      <th>Import Date</th>
      <th class="import-table-download">Download</th>
    </tr>
  </thead>
  <tbody>
    <% imports.each do |import| %>
      <tr>
        <td><%= import.filename %></td>
        <td><%= import.created_at.strftime('%m/%d/%Y') %></td>
        <td class="import-table-download">
          <a class="file-download-button btn-floating btn waves-effect waves-light brown lighten-2"><i class="material-icons">play_for_work</i></a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
