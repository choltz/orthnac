<div class="import-page">
  <div class="row">
    <div class="col s11 offset-s10">
      <%= render 'file/upload' %>
    </div>
    <div class="col s12">
      <div class="card">
        <div class="card-content">
          <span class="card-title">Import History</span>

          <%= render 'import_table', imports: @imports %>
        </div>
      </div>
    </div>
  </div>
</div>

<% deferred_javascript do %>
  app.File();
<% end %>
